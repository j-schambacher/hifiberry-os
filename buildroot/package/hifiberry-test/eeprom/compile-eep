#!/bin/sh
eepmake digipro.txt digipro.eep /boot/overlays/hifiberry-digi-pro.dtbo
eepmake dacplus.txt dacplus.eep /boot/overlays/hifiberry-dacplus.dtbo
eepmake digiplus.txt digiplus.eep /boot/overlays/hifiberry-digi.dtbo
eepmake dacplusadcpro.txt dacplusadcpro.eep /boot/overlays/hifiberry-dacplusadcpro.dtbo
eepmake dacplusadc.txt dacplusadc.eep /boot/overlays/hifiberry-dacplusadc.dtbo
eepmake dac2hd.txt dac2hd.eep /boot/overlays/hifiberry-dacplushd.dtbo
eepmake amp100.txt amp100.eep /boot/overlays/hifiberry-dacplus.dtbo
eepmake amp3.txt amp3.eep /boot/overlays/hifiberry-amp3.dtbo
eepmake dac2pro.txt dac2pro.eep /boot/overlays/hifiberry-dacplus.dtbo
eepmake digi2pro.txt digi2pro.eep /boot/overlays/hifiberry-digi-pro.dtbo
